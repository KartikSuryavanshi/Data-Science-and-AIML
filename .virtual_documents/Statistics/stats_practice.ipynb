#z-score computation

outliers=[]
def detect_outliers(data):
    threshold=3 ##3rd standard deviation
    mean=np.mean(data)
    std=np.std(data)

    for i in data:
        z_score=(i-mean)/std
        if np.abs(z_score)>threshold:
           outliers.append(i)
    return outliers
        


detect_outliers(data)


